                                    <td scope="col" class="right-allign"><apex:outputText value="{0,date,M/d/yyyy}"><apex:param value="{!termingProducts.Opportunity.EffectiveDate__c}"/></apex:outputText></td>
                                </tr>
                        </apex:repeat>
                        
                    </tbody>
                </table>
            </apex:outputText>
        </div>
        <br/>
        
        <div class="sub-heading">
            Open Membership Activities
        </div>
        <br/>
