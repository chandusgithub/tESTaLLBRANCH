<apex:page controller="RenewalChecklistPdfController" showHeader="false" sidebar="false" applyHtmlTag="false" applyBodyTag="false" standardStylesheets="false" renderAs="advanced_pdf">
    <head>
        <style>
            @page{            
            margin:  .8cm;
            @bottom-right {
            content: "Page " counter(page) " of " counter(pages);
            font-size:12px;
            padding-right:12px;
            }
            }
            body{
            font-family: Arial;
            font-size: 13px;
            line-height:120%;
            }
            table{
            border-collapse: collapse;
            border:1px solid black;
            }
            table tr td{
            border:1px solid black;
            }
            table tr th{
            border:1px solid black;
            }
            .customTable,.customTable td {  <!-- CR - 3826 Vignesh -->
            border:0 !important;
            }   
            .heading {
            font-size:20px;
            font-weight:bold;
            width:100%;
            padding:0px;
            margin-top:15px;
            border-bottom:1px solid black;
            color:#193e63;
            line-height:130%;
            }
            
            .sub-heading {
            /*font-size:15px;
            font-weight:bold;
            background-color:#9cb3dd;
            width:100%;
            padding:3px;
            border-radius: 2px;*/
            background: #8aa6d9;
            padding: 9px 13px;
            width: 100%;
            font-size: 14px;
            border:1px solid #8aa6d9;
            border-radius:9px;
            font-weight:bold;
            }
            
            .right-allign{
            text-align: right;
            }
            
            .cell-padding{
            padding-left:5px
            }
            
            .page-break{
            page-break-inside:avoid; 
            page-break-after:auto}
            
        </style>
        
    </head> 
    <body>
        <div style="float:right;">
            <img src="{!$Resource.SoldCaseCheckListLogo}"/>
        </div>
        <br/><br/>
        <div class="heading" style="margin-bottom:0px">            
            <p style="margin-bottom:5px">
                {!accountName} Product Change Checklist
            </p>
            <p style="font-size:13px;margin-bottom:0px;margin-top:0px">
                <!--apex:outputText value="{0,date,M/d/yyyy h:mm a}"><apex:param value="{!formatedDate}"/></apex:outputText-->
                {!formatedDate}
            </p>
        </div>
        <!-- CR - 3826 Vignesh -->
         <table class = "customTable" border="0" width="100%">
             <tr>
                 <td width="60%">
                     <p style="color:#193e63;">
                         <b>Total Current Medical UHC Members:<apex:outputText value="{0,number, ###,###}"><apex:param value="{!totalCurrentUHCMembers}"/></apex:outputText></b>                         
                     </p>  
                 </td>
                 <td width="40%">
                     <p style="color:#193e63;">           
                         <b style='text-align:right;'>Surest Members:<apex:outputText value="{0,number, ###,###}"><apex:param value="{!totalSurestMembers}"/></apex:outputText></b>                            
                     </p>
                 </td>
             </tr>
        </table>
       <!-- <p style="color:#193e63;">
            <b>Total Current Medical UHC Members:<apex:outputText value="{0,number, ###,###}"><apex:param value="{!totalCurrentUHCMembers}"/></apex:outputText></b>          
        </p>  -->
        <div class="sub-heading">
            New Products Sold or NBEA Growth on Existing Products
        </div>
        <br/>
        <div>
            <apex:outputText rendered="{!vfPrintData.detailExistingProductsList.size==0}" >
                <b style='width:100%;text-align:center;float:left;'>{!$label.Renewal_Checklist_no_new_products}</b><br/>
            </apex:outputText>
            
            <apex:outputText rendered="{!vfPrintData.detailExistingProductsList.size>0}" >
                <table style="width:100%" cellpadding="10" cellspacing="0" border="1" width="100%">
                    <colgroup>
                        <col style = "width:40%;height:30px;"/>
                        <col style = "width:20%;height:30px;"/>
                        <col style = "width:20%;height:30px;"/>
                        <col style = "width:20%;height:30px;"/>
                    </colgroup>
                    <thead>
                        <tr class="page-break" style="background:#f6f4f4;">
                            <th scope="col">
                                Product
                            </th>
                            <th scope="col" class="right-allign">
                                Product Selection
                            </th>
                            <th scope="col" class="right-allign">
                                Sold New Mbrs
                            </th>
                            <!--th scope="col">
                                Existing Mbrs Retained
                            </th-->
                            <th scope="col" class="right-allign">
                                Effective Date
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        
                        <apex:repeat value="{!vfPrintData.detailExistingProductsList}" var="existingProduct">
                                <tr class="page-break">
                                    <td scope="col">{!existingProduct.Product2.Name}</td>
                                    <td scope="col" class="right-allign">{!existingProduct.Buyup_Product_Selection__c}</td>
                                    <td scope="col" class="right-allign"><apex:outputText value="{0,number, ###,###}"><apex:param value="{!existingProduct.Net_Results__c}"/></apex:outputText></td>
                                    <!--td scope="col" class="right-allign"><apex:outputText value="{0,number, ###,###}"><apex:param value="{!existingProduct.Existing_Members_Retained__c}"/></apex:outputText></td-->
                                    <td scope="col" class="right-allign"><apex:outputText value="{0,date,M/d/yyyy}"><apex:param value="{!existingProduct.Opportunity.EffectiveDate__c}"/></apex:outputText></td>
                                </tr>
                        </apex:repeat> 
                    </tbody>
                </table>
            </apex:outputText>
        </div>
        <br/>
        
        <div class="sub-heading">
            Fully or Partially Terming Products
        </div>
        <br/>
        <div>
            <apex:outputText rendered="{!vfPrintData.detailTermingProductsList.size==0}" >
