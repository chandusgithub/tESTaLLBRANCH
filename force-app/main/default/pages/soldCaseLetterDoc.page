<apex:page showHeader="false" sidebar="false" applyBodyTag="false" applyHtmlTag="false" controller="SoldCaseCheckListPrintCtrl" language="en-US"
           contentType="{!ContentTypeValue}">
    <apex:includeLightning />
    <div id="LightningComponentid"/>
    <head>
       <meta content="text/html;charset=UTF-8" />
    </head>  
    
    <html>      
      <body>
          <!-- Image Logo (Upload the logo to document setup and copy the link and paste it in custom label and use of logo to display in doc) -->
          <table style="width: 100%; text-align: center;">
              <tr>
                  <td style="text-align: left;">
                      <img src="{!$Label.SoldCaseLetterSurestImage}" width="120" height="40" />
                  </td>
                  <td style="text-align: center;">
                      <img src="{!$Label.SoldCaseLetterUnitedImage}" width="120" />                   
                  </td>
                  <td style="text-align: right;">
                      <img src="{!$Label.SoldCaseLetterOptumImage}" width="120" />
                  </td>
              </tr>
              <!-- <td style="width:50%" align="right">
<img src="{!$Resource.SoldCaseLetterImage1}" />
</td>  -->
          </table>
  
         <br/>
         <br/>
         <!-- MA Name and Header of the Document -->  
         <div style="height: 40px; text-align: center; font-weight: bold; font-size: 15px; font-family: Arial; width: 80%;">
             {!soldCaseLetterWrapper.soldCaseLetterQuery.Name}
         </div>  
         <br/>           
         <div style="text-align: center; font-size: 10px; font-family: Arial; width: 100%; font-size: 16px;">
             <b>Sold Case Letter</b><br/>
             <span style="font-size: 12px;">As of {!FormattedDate}</span>
         </div>
          
         <br/>
         <br/>
          
          <div style="font-size: 13px; font-family: Arial; width: 100%;display: block;text-align: justify;">
              Thank you for choosing <b>UnitedHealthcare</b> to partner with <b>{!soldCaseLetterWrapper.soldCaseLetterQuery.Account.Name}</b> for Medical services for an <b>effective date of {!FormattedEffectiveDate}</b>. We are excited to start the implementation process and would like to outline what we have included in our proposal and believe to be sold, and those provisions where we await your decision.             
         </div>
          
          <br/>
          <br/>
         <!-- Client Overview Table Section Start -->  
          <table style="width: 100%; border-collapse: collapse; border: 1px solid #000; font-family: Arial;">
              <tr>
                  <td style="color:#f2f4f7;background: #002060;padding: 5px;font-size: 14px; font-weight: bold;border:1px solid #000; text-align: center;" colspan="2">
                      Client Overview
                  </td>
              </tr> 
              <tr>
                  <td align="Left" style="width:30%; font-size: 12px; font-weight: bold;border: 1px solid #000; height: 3px; padding-left: 5px;">Company</td>
                  <td align="Left" style="width:70%; font-size: 12px; border: 1px solid #000; font-weight: normal; word-break:break-all; white-space:nowrap; padding-left: 5px;">&nbsp;{!soldCaseLetterWrapper.soldCaseLetterQuery.Account.Name}</td>
              </tr>
              <tr>
                  <td align="Left" style="width:30%; font-size: 12px; font-weight: bold;border: 1px solid #000; height: 3px; padding-left: 5px;">Owner</td>
                  <td align="Left" style="width:70%; font-size: 12px; border: 1px solid #000; font-weight: normal; word-break:break-all; white-space:nowrap; padding-left: 5px;">&nbsp;{!soldCaseLetterWrapper.soldCaseLetterQuery.Owner.Name}</td>
              </tr>
              <tr>
                  <td align="Left" style="width:30%; font-size: 12px; font-weight: bold;border: 1px solid #000; height: 3px; padding-left: 5px;">Effective Date</td>
                  <td align="Left" style="width:70%; font-size: 12px; border: 1px solid #000; font-weight: normal; word-break:break-all; padding-left: 5px;">&nbsp;
                      <apex:outputText value="{0,date,M/d/yyyy}">
                          <apex:param value="{!soldCaseLetterWrapper.soldCaseLetterQuery.EffectiveDate__c}" />                            
                      </apex:outputText>
                  </td>
              </tr>
              <tr>
                  <td align="Left" style="width:30%; font-size: 12px; font-weight: bold;border: 1px solid #000; height: 3px; padding-left: 5px;">Consulting Firm</td>
                  <td align="Left" style="width:70%; font-size: 12px; border: 1px solid #000; font-weight: normal; word-break:break-all; padding-left: 5px;">&nbsp;{!soldCaseLetterWrapper.soldCaseLetterQuery.Primary_Consulting_Firm__r.Name}</td>
              </tr>
              <tr>
                  <td align="Left" style="width:30%; font-size: 12px; font-weight: bold;border: 1px solid #000; height: 3px; padding-left: 5px;">Financials</td>
                  <td align="Left" style="width:70%; font-size: 12px; border: 1px solid #000; font-weight: normal; word-break:break-all; padding-left: 5px;">&nbsp;</td>
              </tr>
          </table>
          <br/><br/>
          <!-- Client Overview Table Section End -->
           
          
           
           <!-- Medical Section Start ---->           
          <table  style="width: 100%; border-collapse: collapse; border: 1px solid #000; font-family: Arial;" >
              <tr>
                  <td colspan="2" style="color:#f2f4f7;background: #002060;padding: 5px;font-weight: bold;font-size: 14px;border:1px solid #000;text-align: center; font-family: Arial;width:100%;">
                      Products and Services 
                  </td>
              </tr>
              
              <td colspan="2" style="height: 1px; background-color: #808080; padding: 0; border: none;"></td>
              
              <apex:outputPanel layout="none" rendered="{!IF(soldCaseDetails.medilineitem != null && soldCaseDetails.medilineitem.size >0, true,false)}">
                  <tr>
                      <td colspan="2" align="left" style="border:1px solid #000;font-weight:bold; color:#f2f4f7;background: #002060; font-family: Arial; font-size: 14px; padding-left: 3px; height:10px;">
                          Medical Products
                      </td>
                  </tr>                
                  <tr>
                      <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:15px; font-family: Arial; font-size: 12px;" >
                          Tiered Benefits  (Not applicable to Nexus)
                      </td>
                      <td style="width:20%; border-bottom:1px solid #000;border-right:1px solid #000; font-family: Arial; font-size: 12px; padding-right: 5px; word-break:break-all;text-align:center;">&nbsp;
                          {!soldCaseDetails.sccData.Tiered_Benefits_Excluding_Nexus__c}
                      </td>
                  </tr>
                  <tr >
                      <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:15px; font-family: Arial; font-size: 12px;" >
                          % of Rx Rebates retained by UHC
                      </td>
                      <td style="width:20%; border-bottom:1px solid #000;border-right:1px solid #000; font-family: Arial; font-size: 12px; padding-right: 5px; word-break:break-all;text-align:center;">&nbsp;
                          {!soldCaseDetails.sccData.Rx_rebates_under_medical__c}
                      </td>
                  </tr>
                  
                  <apex:repeat value="{!medProductFamilyVsLineItems}" var="mediFamily">
                      <tr >
                          <td colspan="2" align="left" style="border:1px solid #000;font-weight:bold; font-family: Arial; font-size: 12px; padding-left:5px;" >
                              {!mediFamily}
                          </td>
                      </tr>
                      <apex:repeat value="{!medProductFamilyVsLineItems[mediFamily]}" var="oppLineItem">
                          <tr >
                              <td align="left" style="border:1px solid #000;border-right:0px;padding-left:35px; font-family: Arial; font-size: 12px;word-break:break-all; " >
                                  <table style="width:100%; border-collapse:collapse;font-family: Arial; font-size: 12px;">
                                      <tr>
                                          <td style="text-align: left; border: none;width:80%;">
                                              {!oppLineItem.Product2.Name}
                                          </td>                                         
                                          <td style="text-align: left; border: none; width:20%;">
                                              <apex:outputPanel rendered="{!NOT(ISBLANK(oppLineItem.Level_2_Option__c))}">
                                                  {!oppLineItem.Level_2_Option__c}
                                              </apex:outputPanel>
                                              <apex:outputPanel rendered="{!NOT(ISBLANK(oppLineItem.Level2_Info__c))}">
                                                  {!oppLineItem.Level2_Info__c}
                                              </apex:outputPanel>
                                          </td>
                                      </tr>
                                  </table>
                              </td>
                              <td align="right" style="width:50%;border-bottom:1px solid #000;border-right:1px solid #000; font-family: Arial; font-size: 12px; word-break:break-all;">                                                                     
                                  <div style="width:25%;min-height:20px;float:right;margin:0;padding:0;text-align:center; font-family: Arial; font-size: 12px; padding-right: 5px;">
                                      <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Medical__c != null && soldCaseDetails.oppdata.Disposition_Medical__c == 'Sold',true,false)}">
                                          Sold 
                                      </apex:outputPanel>
                                      <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Medical__c != null && soldCaseDetails.oppdata.Disposition_Medical__c == 'Spin-Off',true,false)}">
                                          Spin-Off 
                                      </apex:outputPanel>
                                      <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Medical__c != null && soldCaseDetails.oppdata.Disposition_Medical__c == 'Transfer In',true,false)}">
                                          Transfer In 
                                      </apex:outputPanel>
                                      <apex:outputPanel rendered="{!IF((soldCaseDetails.oppdata.Disposition_Medical__c==null || (soldCaseDetails.oppdata.Disposition_Medical__c != 'Sold' && soldCaseDetails.oppdata.Disposition_Medical__c != 'Spin-Off' && soldCaseDetails.oppdata.Disposition_Medical__c != 'Transfer In')),true,false)}">
                                          TBD  
                                      </apex:outputPanel>                              
                                  </div>                                    
                              </td>
                          </tr>
                      </apex:repeat>
                  </apex:repeat>                    
              </apex:outputPanel>
          </table>
          <br/>
          <br/>
                   
          <!-- Medical Section End ---->
           
          <!-- Pharmacy Section Start  --->
          <apex:outputPanel layout="none" rendered="{!IF(soldCaseDetails.pharmacylineitem != null && soldCaseDetails.pharmacylineitem.size>0,true,false)}">
              <table  style="width: 100%; border-collapse: collapse; border: 1px solid #000; font-family: Arial;">
                  <tr >
                      <td colspan="2" align="left" style="border:1px solid #000;font-weight:bold; color:#f2f4f7;background: #002060; font-family: Arial; font-size: 14px; padding-left: 3px; height:10px;" >
                          Pharmacy Products
                      </td>                      
                  </tr>
                  
                  <apex:repeat value="{!soldCaseDetails.pharmacylineitem}" var="pharmacylineitem">
                      <tr>
                          <td style="width:80%; border:1px solid #000; border-right:0px; padding-left:35px; font-family: Arial; font-size: 12px;">
                              <table style="width:100%; border-collapse:collapse;font-family: Arial; font-size: 12px;">  <!-- If Level2 Option isRender -->
                                  <tr>
                                      <td style="text-align:left; border:none;width:60%; word-break:break-all;">
                                          {!pharmacylineitem.Product2.Name}
                                      </td>                                     
                                      <td style="text-align:left; border:none;width:40%;">
                                          <table style="border-collapse:collapse;font-family: Arial; font-size: 12px;">
                                          <td style="text-align:left; border:none;width:30%;white-space:normal; word-wrap:break-word;">
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(pharmacylineitem.Level_2_Option__c))}">
                                              {!pharmacylineitem.Level_2_Option__c}
                                          </apex:outputPanel>
                                          </td>
                                          <td style="text-align:right; border:none;width:10%; white-space:nowrap; ">
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(pharmacylineitem.Pharmacy_Carve_Out_DropDown__c))}">
                                              {!pharmacylineitem.Pharmacy_Carve_Out_DropDown__c}
                                          </apex:outputPanel>
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(pharmacylineitem.Level2_Info__c))}">
                                              {!pharmacylineitem.Level2_Info__c}
                                          </apex:outputPanel>
                                          </td>
                                          </table>
                                      </td>
                                  </tr>
                              </table>
                          </td>
                                                    
                          <td align="right" style="width:50%;border:1px solid #000;border-left:0px;font-family: Arial; font-size: 12px; word-break:break-all;">                              
                              <div style="width:20%;min-height:20px;float:right;margin:0;padding:0;text-align:center;font-family: Arial; font-size: 12px;">
                                  <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Pharmacy__c=='Sold',true,false)}">
                                      Sold
                                  </apex:outputPanel>
                                  <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Pharmacy__c=='Spin-Off',true,false)}">
                                      Spin-Off
                                  </apex:outputPanel>
                                  <apex:outputPanel rendered="{!IF(soldCaseDetails.oppdata.Disposition_Pharmacy__c=='Transfer In',true,false)}">
                                      Transfer In
                                  </apex:outputPanel>
                                  <apex:outputPanel rendered="{!IF((soldCaseDetails.oppdata.Disposition_Pharmacy__c!='Sold' && soldCaseDetails.oppdata.Disposition_Pharmacy__c!='Spin-Off' && soldCaseDetails.oppdata.Disposition_Pharmacy__c!='Transfer In'),true,false)}">
                                      TBD
                                  </apex:outputPanel>
                              </div>
                          </td>
                      </tr>
                      <!-- Leve12 Option Display section -->
                      <apex:outputPanel layout="none" rendered="{!IF(pharmacylineitem.Level_2_Option__c == 'Carve in - OptumRx',true,false)}">
                          <tr>
                              <td colspan="2" style="color:#f2f4f7;background: #4F81BD;font-weight: bold;font-size: 12px; font-family: Arial;border:1px solid #000;padding-left: 3px;">
                                  Pharmacy Carve In Programs
                              </td>
                          </tr>                          
                          <tr>                                
                              <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:45px;font-family: Arial; font-size: 12px;">ESP (Enhanced Savings Program) Opt-in</td>
                              <td style="border:1px solid #000;width:20%;font-family: Arial; font-size: 12px;text-align:center; word-break:break-all;">{!pharmacylineitem.ESP_Enhanced_Savings_Program_Opt_in__c}</td>
                          </tr>
                          <tr>
                              <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:45px;font-family: Arial; font-size: 12px;">Price Edge Opt-in</td>
                              <td style="border:1px solid #000;width:20%;font-family: Arial; font-size: 12px;text-align:center; word-break:break-all;">{!pharmacylineitem.Price_Edge_Opt_in__c}</td>
                          </tr>
                          <tr>
                              <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:45px;font-family: Arial; font-size: 12px;">Vital Medication program Opt-in</td>
                              <td style="border:1px solid #000;width:20%;font-family: Arial; font-size: 12px;text-align:center; word-break:break-all;">{!pharmacylineitem.Vital_Medication_program_Opt_in_Opt_out__c}</td>                                
                          </tr>
                          <apex:outputPanel layout="none" rendered="{!IF((pharmacylineitem.ProductCode != 'RX-ONERX' && pharmacylineitem.ProductCode !='RX-SRORBFI' && pharmacylineitem.ProductCode !='RX-SRORBNI'),true,false)}">
                              <tr>  
                                  <td align="left" style="width:80%; border:1px solid #000;border-right:0px;padding-left:45px;font-family: Arial; font-size: 12px;">If Fully Insured include quoted Rx plan number in text box</td>
                                  <td style="border:1px solid #000;width:20%;font-family: Arial; font-size: 12px;text-align:center; word-break:break-all;">{!pharmacylineitem.If_Fully_Insured_include_quoted_Rx_plan__c}</td>
                              </tr> 
                          </apex:outputPanel>                                                  
                     </apex:outputPanel>
                  </apex:repeat>                 
              </table>
              <br/>
              <br/>
          </apex:outputPanel>
          
           <!-- Phramacy Section End -->
           
           
           <!-- Dental Section Start  -->
          <apex:outputPanel layout="none" rendered="{!IF(soldCaseDetails.dentallineitem != null && soldCaseDetails.dentallineitem.size>0,true,false)}">
              <table style="width: 100%; border-collapse: collapse; border: 1px solid #000; font-family: Arial;">
                  <!-- Rendered Dental Product heading traditional  and Surest -->
                  <tr>
                      <td colspan="3" align="left" style="border:1px solid #000;font-weight:bold; color:#f2f4f7;background: #002060; font-family: Arial; font-size: 14px; padding-left: 3px; " >
                          Dental Products
                      </td>                      
                  </tr>
                  
                  <tr>
                      <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, NOT(isSurestSoldDental))}">
                          <td align="left" style="width:80%; border:1px solid #000;border-right:0px;border-right:none;padding-left:25px; font-family: Arial; font-size: 12px;" colspan="2">
                              Dental Clinical Integration
                          </td>
                          <td style="width:20%; border-bottom:1px solid #000;border-right:1px solid #000; font-family: Arial; font-size: 12px; text-align:center;word-break:break-all; white-space:nowrap;">
                              {!soldCaseDetails.oppdata.Bridge2Health_opt_out__c}
                          </td>
                      </apex:outputPanel>
                      <apex:outputPanel layout="none" rendered="{!AND(isSurestSoldDental, NOT(isTradSoldDental))}">
                          <td align="left" style="width:80%; border:1px solid #000;border-right:0px;border-right:none;padding-left:25px; font-family: Arial; font-size: 12px;" colspan="2">
                              Dental Clinical Integration
                          </td>
                          <td style="width:20%; border-bottom:1px solid #000;border-right:1px solid #000; font-family: Arial; font-size: 12px; text-align:center;word-break:break-all; white-space:nowrap;">
                              {!soldCaseDetails.oppdata.Bridge2Health_opt_out__c}
                          </td>
                      </apex:outputPanel>
                      <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, isSurestSoldDental)}">
                          
                          <td align="left" style="width:70%; border:1px solid #000;border-right:0px;border-right:none;padding-left:25px; font-family: Arial; font-size: 12px;">
                              Dental Clinical Integration
                          </td>
                          <td style=" border-bottom:1px solid #000;border-right:1px solid #000;border-right:none; font-family: Arial; font-size: 12px; text-align:right;word-break:break-all; white-space:nowrap;">
                              {!soldCaseDetails.oppdata.Bridge2Health_opt_out__c}
                          </td>
                          <td></td>
                      </apex:outputPanel>
                  </tr>
                  
                  <tr>                     
                      <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, NOT(isSurestSoldDental))}">
                          <td align="left" style="width:80%;border:1px solid #000;border-right:0px;padding-left:35px; font-family: Arial; font-size: 12px;" colspan="2">                          
                          </td>
                          <td style="width:20%; border:1px solid #000; font-weight:bold; text-align:center;font-family: Arial; font-size: 12px;">
                              <i>Traditional</i>
                          </td>
                      </apex:outputPanel>
                      <apex:outputPanel layout="none" rendered="{!AND(isSurestSoldDental, NOT(isTradSoldDental))}">
                          <td align="left" style="width:80%;border:1px solid #000;border-right:0px;padding-left:35px; font-family: Arial; font-size: 12px;" colspan="2">                          
                          </td>
                          <td style="width:20%; border:1px solid #000; font-weight:bold; text-align:center;font-family: Arial; font-size: 12px;">
                              <i>Surest</i>
                          </td>
                      </apex:outputPanel>
                      <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, isSurestSoldDental)}">  
                          <td align="left" style="width:70%;border:1px solid #000;border-right:none;padding-left:35px; font-family: Arial; font-size: 12px;">                          
                          </td>                          
                          <td style="width:15%; border:1px solid #000; font-weight:bold; text-align:center; border-left:0px;font-family: Arial; font-size: 12px;">
                              <i>Traditional</i>
                          </td>
                          <td style="width:15%; border:1px solid #000; font-weight:bold;text-align:center;font-family: Arial; font-size: 12px;">
                              <i>Surest</i>
                          </td>
                      </apex:outputPanel>
                  </tr>
                  
                  <!-- Product Name -->
                  <apex:repeat value="{!soldCaseDetails.dentallineitem}" var="dentalopplineitem">
                      <tr>  
                          <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, NOT(isSurestSoldDental))}">
                              <td style="width:80%; border:1px solid #000; padding-left:35px; font-size: 12px; font-family: Arial;">
                              <table style="width:100%; border-collapse:collapse; font-family: Arial; font-size: 12px;">
                                  <tr>
                                      <td style="text-align:left; border:none;width:80%">
                                          {!dentalopplineitem.Product2.Name}                                       
                                      </td>
                                      <td style="text-align:left; border:none;width:20%">
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level_2_Option__c))}">
                                              {!dentalopplineitem.Level_2_Option__c}
                                          </apex:outputPanel>
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level2_Info__c))}">
                                              {!dentalopplineitem.Level2_Info__c}
                                          </apex:outputPanel>
                                      </td>
                                  </tr>
                              </table>
                          </td>
                          </apex:outputPanel>
                          <apex:outputPanel layout="none" rendered="{!AND(isSurestSoldDental, NOT(isTradSoldDental))}">
                              <td style="width:80%; border:1px solid #000; padding-left:35px; font-size: 12px; font-family: Arial;">
                              <table style="width:100%; border-collapse:collapse; font-family: Arial; font-size: 12px;">
                                  <tr>
                                      <td style="text-align:left; border:none;width:80%">
                                          {!dentalopplineitem.Product2.Name}                                       
                                      </td>
                                      <td style="text-align:left; border:none;width:20%">
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level_2_Option__c))}">
                                              {!dentalopplineitem.Level_2_Option__c}
                                          </apex:outputPanel>
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level2_Info__c))}">
                                              {!dentalopplineitem.Level2_Info__c}
                                          </apex:outputPanel>
                                      </td>
                                  </tr>
                              </table>
                          </td>
                          </apex:outputPanel>
                          <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, isSurestSoldDental)}"> 
                              <td style="width:70%; border:1px solid #000; padding-left:35px; font-size: 12px; font-family: Arial;">
                              <table style="width:100%; border-collapse:collapse; font-family: Arial; font-size: 12px;">
                                  <tr>
                                      <td style="text-align:left; border:none;width:80%">
                                          {!dentalopplineitem.Product2.Name}                                       
                                      </td>
                                      <td style="text-align:left; border:none;width:20%">
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level_2_Option__c))}">
                                              {!dentalopplineitem.Level_2_Option__c}
                                          </apex:outputPanel>
                                          <apex:outputPanel rendered="{!NOT(ISBLANK(dentalopplineitem.Level2_Info__c))}">
                                              {!dentalopplineitem.Level2_Info__c}
                                          </apex:outputPanel>
                                      </td>
                                  </tr>
                              </table>
                          </td>
                          </apex:outputPanel>
                          
                          
                          <!-- Traditional only column --> 
                          <apex:outputPanel layout="none" rendered="{!AND(isTradSoldDental, NOT(isSurestSoldDental))}">
                              <td style="width:20%; border:1px solid #000; text-align: center; font-family: Arial; font-size: 12px; word-break:break-all;" colspan="2">
                                  <apex:outputPanel rendered="{!IF(AND(soldCaseDetails.oppdata.Disposition_Dental__c=='Sold',dentalopplineitem.Buyup_Product_Selection__c!='Surest Only'),true,false)}">
                                      Sold
