                        <!-- <td style="width:70%; text-align: left; border: 1px solid #000; padding-left:5px; font-size: 12px;">{!ebdWrapper.getAccountInfo.Medical_Membership_Penetration__c}</td>-->
                        <td style="width:70%; text-align: left; border: 1px solid #000; padding-left:5px; font-size: 12px;">
                            {!IF(ISBLANK(ebdWrapper.getAccountInfo.Medical_Membership_Penetration__c), 
