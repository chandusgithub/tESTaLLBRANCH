                            <!--{!consultant.Contact.Account.Location__c}-->
                            {!IF(AND(NOT(ISBLANK(consultant.Contact.MailingCity)), 
                            NOT(ISBLANK(consultant.Contact.MailingStateCode))), 
                            consultant.Contact.MailingCity & ', ' & consultant.Contact.MailingStateCode,
                            IF(NOT(ISBLANK(consultant.Contact.MailingCity)), 
                            consultant.Contact.MailingCity, 
