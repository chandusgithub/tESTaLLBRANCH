<apex:page showHeader="false" sidebar="false" applyBodyTag="false" contentType="{!ContentTypeValue}"
           applyHtmlTag="false" controller="EBDController" language="en-US">
    <apex:includeLightning />
    <div id="LightningComponentid" />
