                    </tr>
                <!--</thead>-->
                <apex:variable var="index" value="{!0}" />
                <apex:repeat value="{!ebdWrapper.getClientPriorityDetails}" var="clientPriority">
                    <apex:variable var="index" value="{!index + 1}" />    
                    <tr>
                        <td style="border: 1px solid #000; font-size: 12px; text-align: center; width:5%; vertical-align:top; ">{!index}.</td>    
                        <td style="border: 1px solid #000; text-align:left; font-size: 12px; vertical-align:top;  width:40%; padding-left:5px; padding-bottom:5px; word-wrap:break-word; white-space:normal;">
                            <!--<apex:outputText value="{!IF(ISBLANK(clientPriority.Client_Priority__c),'&nbsp;',clientPriority.Client_Priority__c)}" escape="false"/>-->
                            <apex:outputField value="{!clientPriority.Client_Priority__c}"/>
                            
                            
                        </td>
                        <td style="border: 1px solid #000; text-align:left; font-size: 12px;  vertical-align:top; width:55%; padding-left:5px; padding-bottom:5px; word-wrap:break-word; white-space:normal;">
                            <!--<apex:outputText value="{!IF(ISBLANK(clientPriority.UHG_Strategy__c),'&nbsp;',clientPriority.UHG_Strategy__c)}" escape="false"/>-->
                            <apex:outputField value="{!clientPriority.UHG_Strategy__c}"/>
                            
