                    </tr>
                </apex:repeat>
                <apex:outputPanel rendered="{!ISBLANK(ebdWrapper.getCompanyContactsData)}">
                    <tr>
                        <td colspan="3" style="border: 1px solid #000; text-align:center; font-size: 12px; padding: 6px;">
                            No records to display 
                        </td>
                    </tr>
                </apex:outputPanel>
            </table>
            <br/>
            <br/>
            <!-- key personnel clients end-->
            <table style=" width: 100%; border-collapse: collapse; border: 1px solid #000;  table-layout: fixed; font-family: Calibri; ">
                
