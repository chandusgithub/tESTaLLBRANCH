                                                        <div class="slds-form-element">
                                                            <div class="slds-form-element__control"> 
                                                                <ui:inputSelect class="slds-select" aura:id="NPS_Output_1" multiple="false" change="{!c.onNPSFieldChange}">
                                                                    <aura:iteration items="{!v.NPSFields}" var="row2condition_var">
                                                                        <aura:if isTrue="{!row2condition_var == 'Owner'}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" label="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}" label="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if> 
                                                                    </aura:iteration>
                                                                </ui:inputSelect>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </th>
                                                <th scope="row" data-label="Condition">
                                                    <div class="slds-truncate" title="Condition">
                                                        <div class="slds-form-element">
                                                            <div class="slds-form-element__control">
                                                                <ui:inputSelect class="slds-select" aura:id="NPS_Filter_1" multiple="false" >
                                                                    <aura:if isTrue="{!v.type1 == 'NUMBER'}"> 
                                                                        <aura:iteration items="Begins With,Equals to,Contains" var="row2condition_var" indexVar="indx">
                                                                            <aura:if isTrue="{!indx == 0}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>    
                                                                        </aura:iteration>
                                                                    </aura:if>
                                                                    <aura:if isTrue="{!v.type1 == 'STRING'}">
                                                                        <aura:iteration items="Begins With,Equals to,Contains" var="row2condition_var"> 
                                                                            <aura:if isTrue="{!indx == 0}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>
                                                                        </aura:iteration>
                                                                    </aura:if>
                                                                    <aura:if isTrue="{!v.type1 == 'PICKLIST'}">
                                                                        <aura:iteration items="Equals to" var="row2condition_var"> 
                                                                            <aura:if isTrue="{!indx == 0}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>
                                                                        </aura:iteration>
                                                                    </aura:if>
                                                                    <aura:if isTrue="{!v.type1 == 'DATE'}">
                                                                        <aura:iteration items="Equals to,On or before,On or after" var="row2condition_var"> 
                                                                            <aura:if isTrue="{!indx == 0}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>
                                                                        </aura:iteration>
                                                                    </aura:if>
                                                                </ui:inputSelect>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </th>
                                                <th scope="row" data-label="Filter Value(s)">
                                                    <div class="slds-truncate" title="Filter Value(s)">
                                                        <div class="slds-form-element">                                    
                                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                                                <aura:if isTrue="{!v.type1 == 'NUMBER'}">
