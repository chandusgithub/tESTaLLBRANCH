                                                            <div class="slds-form-element__control">
                                                                <ui:inputSelect class="slds-select" aura:id="NPS_Output_2" multiple="false" change="{!c.onNPSFieldChange}">
                                                                    <aura:iteration items="{!v.NPSFields}" var="row2condition_var">
                                                                        <ui:inputSelectoption text="{!row2condition_var}" label="{!row2condition_var}"/> 
                                                                    </aura:iteration>
                                                                </ui:inputSelect>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </th>
                                                <th scope="row" data-label="Condition">
                                                    <div class="slds-truncate" title="Condition">
                                                        <div class="slds-form-element">
                                                            <div class="slds-form-element__control">
                                                                <ui:inputSelect class="slds-select" aura:id="NPS_Filter_2" multiple="false" >
                                                                    <aura:if isTrue="{!v.type2 == 'NUMBER'}"> 
