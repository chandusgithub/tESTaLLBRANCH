                                                </tr>
                                                <aura:set attribute="else">
                                                    <aura:iteration items="{!v.accountSearchList}" var="accountSearchListobj" indexVar="index">
                                                        <c:Template_Design_Table aura:id="eachCheckBox" accountSearchArray="{!accountSearchListobj}" accId="{!accountSearchListobj.Id}" accountSearchArrayIndex="{!accountSearchListIndex}" index="{!index+v.serialNumber}" objNameToDisplay="{!v.objectName}" selectedIds="{!v.selectedRefAcctsIds}" NPSsearchCriteriaSection="{!v.NPSsearchCriteriaSection}"></c:Template_Design_Table>
                                                    </aura:iteration>
                                                </aura:set>
                                            </aura:if>
                                        </tbody>
                                    </table>
                                </ui:scrollerWrapper>
                            </div>
