                        <aura:if isTrue="{!v.isSearchBtnClicked}">
                            <aura:if isTrue="{!v.isSearchedResultEmpty}">
                                
                                <aura:set attribute="else">
                                    <div class="slds-grid slds-wrap slds-size_1-of-1">
                                        <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium">
