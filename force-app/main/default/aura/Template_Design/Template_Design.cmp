                                                <th scope="col"><div class="slds-truncate" title="Field">Field</div></th>
                                                <th scope="col"><div class="slds-truncate" title="Condition">Condition</div></th>
                                                <th scope="col"><div class="slds-truncate" title="Filter Value(s)">Filter Value(s)</div></th>
                                                <th class="slds-cell-shrink" scope="col"><div class="slds-truncate" title="Row Number"></div></th>
                                                <th scope="col"><div class="slds-truncate" title="Field">Field</div></th>
                                                <th scope="col"><div class="slds-truncate" title="Condition">Condition</div></th>
                                                <th scope="col"><div class="slds-truncate" title="Filter Value(s)">Filter Value(s)</div></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr class="slds-hint-parent">
                                                <td class="slds-cell-shrink" data-label="Sl.No">
                                                    <div class="slds-truncate" title="Sl.No"></div>
                                                </td>
                                                <th scope="row" data-label="Field">
