                                <div class="slds-spinner__dot-a"></div>
                                <div class="slds-spinner__dot-b"></div>
                            </div>
                        </div>
                    </aura:if>
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-p-horizontal_medium template-opt slds-p-vertical_small">                            
                            <div class="slds-form-element">
                                <div class="slds-form-element__control">
                                    <label class="slds-form-element__label">Select the Template : </label>
                                    <ui:inputSelect class="slds-select dropDown-auto-width" aura:id="objType" multiple="false" change="{!c.onObjChange}">
                                        <aura:iteration items="{!v.objType}" var="objType_var"> 
                                            <ui:inputSelectoption text="{!objType_var}"/>
                                        </aura:iteration>
                                    </ui:inputSelect>
                                </div>
                            </div>
                        </div>
                        <aura:if isTrue="{!v.NPSsearchCriteriaSection}">
                            <div aura:id='NPSsearchCriteriaSection' class='slds-size_1-of-1 slds-grid slds-wrap template-contents slds-grid_vertical-align-center'>
                                <div class="slds-size_1-of-1 slds-p-horizontal_medium slds-p-vertical_small">
                                    <h3 class="table-title">Enter Search Criteria (Show results where) :</h3>
                                </div>
                                <div class="slds-size_1-of-1">
                                    <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal search-table">
