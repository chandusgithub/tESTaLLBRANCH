                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>    
                                                                        </aura:iteration>
                                                                    </aura:if>
                                                                    <aura:if isTrue="{!v.type2 == 'STRING'}">
                                                                        <aura:iteration items="Begins With,Equals to,Contains" var="row2condition_var"> 
                                                                            <aura:if isTrue="{!indx == 0}">
                                                                            <ui:inputSelectoption text="{!row2condition_var}" value='true'/>
                                                                            <aura:set attribute="else">
                                                                                <ui:inputSelectoption text="{!row2condition_var}"/>
                                                                            </aura:set>
                                                                        </aura:if>
