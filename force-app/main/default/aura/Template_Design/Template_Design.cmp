                                                                </aura:if>
                                                                <aura:if isTrue="{!v.type1 == 'SEARCH'}">
                                                                    <div onclick="{!c.openOptionsPopup}" data-record="search1">
                                                                        <a href="" class="noedit-onlysearch">
                                                                            <lightning:icon iconName="utility:search" size="xx-small"></lightning:icon>
                                                                        </a>
                                                                    </div>
                                                                    <ui:inputText aura:id="NPS_Input_1" class="slds-input avoid-click" value="" updateOn="keyup" keyup="{!c.searchForNPSActionGoButton}" placeholder="Filter Value(s)" required="true"/>
                                                                </aura:if>
                                                                <aura:if isTrue="{!v.type1 == 'STRING'}">
                                                                    <ui:inputText aura:id="NPS_Input_1" class="slds-input" value="" updateOn="keyup" keyup="{!c.searchForNPSActionGoButton}" placeholder="Filter Value(s)" required="true"/>
                                                                </aura:if>
                                                                <aura:if isTrue="{!v.type1 == 'PICKLIST'}">
                                                                    <ui:inputSelect class="slds-select" aura:id="NPS_Input_1" multiple="false" >
                                                                        <aura:iteration items="{!v.type1Values}" var="type1Values_var"> 
                                                                            <aura:if isTrue="{!type1Values_var == ''}">
                                                                                <ui:inputSelectOption text="{!type1Values_var}" value="true" /> 
                                                                                <aura:set attribute="else">
                                                                                    <ui:inputSelectoption text="{!type1Values_var}"/>
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </aura:iteration>
                                                                    </ui:inputSelect>
                                                                </aura:if>
                                                                <aura:if isTrue="{!v.type1 == 'DATE'}">
                                                                    <ui:inputDate aura:id="NPS_Input_1" format="M/d/yyyy" label="" value="" displayDatePicker="true"/>
                                                                </aura:if>
                                                            </div>
                                                        </div>
                                                    </div>
