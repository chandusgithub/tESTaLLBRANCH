<aura:component >
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    <aura:attribute name="contactOfTypeConsultantObj" type="Contact[]"/>
    <aura:attribute name="officeLocation" type="String"/>
    
    <aura:registerEvent name="searchConsultantModalTableEvent" type="c:Consultant_History_Consulting_Firm_Model_TableBody_Event"/>
    <tr class="slds-hint-parent">
        <th scope="row" data-label="">
            <div class="slds-truncate" title="">
                <a data-record="{!index}" onclick="{!c.Select}">{!$Label.c.Select}</a>
            </div>
        </th>
        <th scope="row" data-label="Last Name">
            <div class="slds-truncate" title="Last Name">{!v.contactOfTypeConsultantObj.LastName}</div>
        </th>
        <th scope="row" data-label="First Name">
            <div class="slds-truncate" title="First Name">{!v.contactOfTypeConsultantObj.FirstName}</div>
        </th>
        <th scope="row" data-label="Job Title">
            <div class="slds-truncate" title="Job Title">{!v.contactOfTypeConsultantObj.Title}</div>
        </th>
        <th scope="row" data-label="Account Name">
            <div class="slds-truncate" title="Consulting Firm">{!v.contactOfTypeConsultantObj.Account.Name}</div>
        </th>
        <th scope="row" data-label="Office Location">
            <div class="slds-truncate" title="Office Location">{!v.officeLocation}</div>
        </th>
    </tr>
</aura:component>