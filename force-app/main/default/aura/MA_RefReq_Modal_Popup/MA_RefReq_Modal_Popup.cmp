    <aura:attribute name="isFirstConditionPresent" type="boolean" default="false"/>
    
    <aura:attribute name="isOrCondition" type="boolean" default="false"/>
    <aura:attribute name="isAndCondition" type="boolean" default="false"/>
    <aura:attribute name="isCustomLogic" type="boolean" default="false"/>
    
    <aura:attribute name="filterQueries" type="String[]" default="[]" />
    
    <aura:attribute name="filterVals" type="Integer" default="3"/>
    
    <aura:attribute name="selectedRefAcctsIds" type="String[]"/>
    <aura:attribute name="checkBoxCount" type="Integer" default="0"/>
    
    <aura:attribute name="dynamicComponentsByAuraId" type="Object" />
    <aura:attribute name="dynamicComponentAuraId" type="String"/>
    
    <aura:handler name="modalCmpCloseEvent1" event="c:Modal_Closing_RR_Event" action="{!c.modelCloseComponentEvent}"/>
    
