    <aura:attribute name='page' type='Integer'/>
    <aura:attribute name='pages' type='Integer'/>
    <aura:attribute name='total' type='Integer'/>    
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    
    <aura:attribute name="client_Ref_dropDownVals" type="String[]"/>
    <aura:attribute name="condition" type="String[]" default="Equal to, Not equal to, On or before, On or after"/>    
    <aura:attribute name="Search_Select_DropDown" type="String[]" default="Consultants associated with the Account,All Consultants"/>
    
    <aura:attribute name="conditiontest" type="String[]"/>
    
    
    <aura:attribute name="condition1" type="String[]" default="Contains at least one value"/>
    <aura:attribute name="condition2" type="String[]" default="Contains at least one value, Contains all values"/>
    <aura:attribute name="condition3" type="String[]" default="Equal to"/>
    <aura:attribute name="condition4" type="String[]" default="Equal to, Not equal to, On or before, On or after"/>
    <aura:attribute name="condition5" type="String[]" default="Is checked, Is Not checked"/>
    <aura:attribute name="condition6" type="String[]" default="Equal to, Not Equal to, Begins with, Less than, Greater than, Contains any"/>
    
    <aura:attribute name="row1condition" type="String[]" default="Contains at least one value"/>
    <aura:attribute name="row2condition" type="String[]" default="Contains at least one value, Contains all values"/>
    <aura:attribute name="row3condition" type="String[]" default=""/>
    <aura:attribute name="row4condition" type="String[]" default=""/>
    <aura:attribute name="row5condition" type="String[]" default=""/>
    <aura:attribute name="row6condition" type="String[]" default=""/>
    <aura:attribute name="row7condition" type="String[]" default=""/>
    
    <aura:attribute name="type1" type="String" default="search"/>
    <aura:attribute name="type2" type="String" default="search"/>
    <aura:attribute name="type3" type="String" default="text"/>
    <aura:attribute name="type4" type="String" default="text"/>
    <aura:attribute name="type5" type="String" default="text"/>
    <aura:attribute name="type6" type="String" default="text"/>
    <aura:attribute name="type7" type="String" default="text"/>
    
    <aura:attribute name="type1Values" type="String[]" default=""/>
    <aura:attribute name="type2Values" type="String[]" default=""/>
    <aura:attribute name="type3Values" type="String[]" default=""/>
    <aura:attribute name="type4Values" type="String[]" default=""/>
    <aura:attribute name="type5Values" type="String[]" default=""/>
    <aura:attribute name="type6Values" type="String[]" default=""/>
    <aura:attribute name="type7Values" type="String[]" default=""/>
    
    <aura:attribute name="accountRecordTypeNames" type="String[]" />
    <aura:attribute name="accountSubTypeNames" type="String[]" />
    <aura:attribute name="hqStates" type="String[]" />
    
    <aura:attribute name="prodBuyUpProgs" type="String[]" />
    <aura:attribute name="CMSCEusers" type="String[]" />
    <aura:attribute name="CVGValues" type="String[]" />
    <aura:attribute name="industryDDValues" type="String[]" />
    <aura:attribute name="LeasedNetworkValues" type="String[]" />
    <aura:attribute name="PrivateExValues" type="String[]" />
    <aura:attribute name="ReasonForAllValues" type="String[]" />
    
    <aura:registerEvent name="accountConsultantSelectSearch" type="c:Account_Consultant_Select_Search_Event"/>
    
    <aura:registerEvent name="addRefAccountsEvent" type="c:addRefAccountsEvent"/>
    
    <aura:handler name="refAcctsPopUpChildEvent" event="c:refAcctsPopUpChildEvent" action="{!c.addSelectedRefAccounts}"/>
    {!v.body} 
    <div role="alertdialog" tabindex="-1" aura:id="FilterSearch" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide modal-sm">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse" onclick="{!c.closeErrorMsg}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">Error</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p style="text-align:center;">{!$Label.c.Search_Criteria_Error}</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral buttonShadowBorder" onclick="{!c.closeErrorMsg}">{!$Label.c.OK}</button>
            </div>
        </div>
    </div>
    
    <div class="slds-backdrop slds-backdrop--open slds-hide" aura:id="FilterSearch"></div>
    
    <div role="alertdialog" tabindex="-1" aura:id="ErrorMessage" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse" onclick="{!c.closeErrorModal}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">Error</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p style="text-align:center;">{!v.ErrorMessage}</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral buttonShadowBorder" onclick="{!c.closeErrorModal}">{!$Label.c.OK}</button>
            </div>
        </div>
    </div>
    
    <div class="slds-backdrop slds-backdrop--open slds-hide" aura:id="ErrorMessage"></div>
    
    <div role="alertdialog" tabindex="-1" aura:id="CustomLogicError" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide modal-sm">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse" onclick="{!c.closeErrorMsg1}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">Error</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p style="text-align:center;">{!v.customLogicErrorMsg}</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral buttonShadowBorder" onclick="{!c.closeErrorMsg1}">{!$Label.c.OK}</button>
            </div>
        </div>
    </div>
    
    <div class="slds-backdrop slds-backdrop--open slds-hide" aura:id="CustomLogicError"></div>
    
    <lightning:notificationsLibrary aura:id="notifLib"/>
    
    <div class="slds-grid slds-wrap slds-is-relative">
        <aura:if isTrue="{!v.isSpinnertoLoad}">    
            <div class="slds-spinner_container" aura:id="spinner1"></div>
        </aura:if>
        
        <section aura:id="searchSection" class="slds-accordion__section slds-is-open slds-size_1-of-1 slds-p-around_none">
            <div class="slds-accordion__summary slds-grid slds-wrap slds-m-bottom_none">
                <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_small">
                    <aura:if isTrue="{!v.isSpinnertoLoad}">
                        <div aura:id="spinner" class="dotsSpinnerContainer slds-is-relative">
                            <div class="dotsSpinnerContent slds-is-relative slds-p-around_medium">
                                <span class="slds-assistive-text">Loading</span>
                                <div role="status" class="slds-spinner slds-spinner_brand slds-spinner_medium">                                
                                    <div class="slds-spinner__dot-a"></div>
                                    <div class="slds-spinner__dot-b"></div>
                                </div>
                            </div>
                        </div>
                    </aura:if>
                    <h3>
                        <aura:if isTrue="{!v.expandCollapseIcon}">
                            <!--Search Results for : {!v.searchedQuery}-->
                            Search Results :
                            <aura:set attribute="else">
                                Enter Search Criteria (Show results where) :
                            </aura:set>
                        </aura:if>
                    </h3>
                </div>
                <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-p-horizontal_medium slds-p-vertical_small slds-grid slds-grid_align-end slds-grid_vertical-align-center">
                    <aura:if isTrue="{!v.expandCollapseIcon}">
                        <div class="slds-is-relative helptext show-md-up slds-p-horizontal_x-small">
                            <c:tooltip helpLink="Help" helpText="Click Close to go back to the Search Criteria."/>
                        </div>
                        <ui:button class="slds-button slds-button_neutral" press="{!c.closeSearchResultSection}">Close</ui:button>
                        <aura:if isTrue="{!v.isSearchedResultEmpty == false}">
                            <ui:button class="slds-button slds-button_brand" aura:id="saveRefAcc" disabled="true" press="{!c.saveRefAcctsResult}">Save</ui:button>
                            <ui:button class="slds-button slds-button_brand" aura:id="downloadCSV" press="{!c.downloadFile}">Download</ui:button>
                            <aura:if isTrue="{!v.isSearchBtnClicked}">
                                <aura:if isTrue="{!v.pages > 1}">
                                    <div class="top-pagination slds-p-left_medium slds-m-left_medium">
                                        <c:Pagination2 page="{!v.page}" pages="{!v.pages}" total="{!v.total}"/>
                                    </div>
                                </aura:if>
                            </aura:if>
                        </aura:if>
                        
                    </aura:if>
                </div>
            </div>
            <div class="slds-accordion__content slds-grid slds-wrap">
                <div class="slds-size_1-of-1 slds-grid slds-wrap hidden-md-up search-logic slds-grid_vertical-align-center">
                    <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12">
                        <div class="slds-form form-inline">
                            <div class="slds-form-element radio-btn">
                                <ui:inputRadio aura:id="andRadio" disabled="false" value="true" label="ALL AND" change="{!c.onChangeRadio}"/>
                            </div> 
                            <div class="slds-form-element radio-btn">
                                <ui:inputRadio aura:id="orRadio" disabled="false" value="false" label="ALL OR" change="{!c.onChangeRadio}"/>
                            </div> 
                            <div class="slds-form-element radio-btn">
                                <ui:inputRadio aura:id="customRadio" disabled="false" value="false" label="CUSTOM LOGIC" change="{!c.onChangeRadio}"/>
                            </div> 
                            <div class="slds-form-element">
                                <div aura:id='customLogicDiv' class="slds-form-element__control slds-hide">
                                    <ui:inputText class="slds-input inputToUpper" disabled="false" updateOn="keyup" aura:id="customLogic1" keyup="{!c.customLogicUpdate}" placeholder="Custom e.g.(1 And 2) Or (3 And 4)" />
                                </div>
                            </div>
                            <div class="slds-form-element">
                                <div aura:id='customLogicHelpText' class="slds-form-element__control slds-p-left_small slds-hide">
                                    <c:tooltip helpLink="Custom Logic Example" helpText="Custom Logic example: 1 OR (2 AND 3)"/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12 slds-grid slds-grid_align-end">                
                        <ui:button class="slds-button slds-button_brand" aura:id="searchResults" press="{!c.searchInitiated}">Search</ui:button>
                        <ui:button class="slds-button slds-button_neutral" press="{!c.resetFieldValues}">Reset</ui:button>
                    </div>
                </div>
                <div class="slds-size_1-of-1">
                    <table class="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal search-table">
                        <thead>
                            <tr class="slds-text-title_caps">
                                <th class="slds-cell-shrink" scope="col"><div class="slds-truncate" title="Row Number">Sl.No</div></th>
                                <th scope="col"><div class="slds-truncate" title="Field">Field</div></th>
                                <th scope="col"><div class="slds-truncate" title="Condition">Condition</div></th>
                                <th scope="col"><div class="slds-truncate" title="Filter Value(s)">Filter Value(s)</div></th>
                                <th class="slds-cell-shrink" scope="col"><div class="slds-truncate" title="Row Number">Sl.No</div></th>
                                <th scope="col"><div class="slds-truncate" title="Field">Field</div></th>
                                <th scope="col"><div class="slds-truncate" title="Condition">Condition</div></th>
                                <th scope="col"><div class="slds-truncate" title="Filter Value(s)">Filter Value(s)</div></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="slds-hint-parent">
                                <td class="slds-cell-shrink" data-label="Sl.No">
                                    <div class="slds-truncate slds-p-left_small" title="Sl.No">1</div>
                                </td>
                                <th scope="row" data-label="Field">
                                    <div class="slds-truncate" title="Field">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <ui:inputSelect class="slds-select" aura:id="row1field" multiple="false" change="{!c.onFieldSelectChange}">
                                                    <aura:iteration items="{!v.client_Ref_dropDownVals}" var="client_Ref_DD_var"> 
                                                        <aura:if isTrue="{!client_Ref_DD_var == v.defSearchField}">
                                                            <ui:inputSelectOption text="{!client_Ref_DD_var}" value="true" /> 
                                                            <aura:set attribute="else">
                                                                <ui:inputSelectoption text="{!client_Ref_DD_var}"/>
                                                            </aura:set>
                                                        </aura:if>
                                                    </aura:iteration>
                                                </ui:inputSelect>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row" data-label="Condition">
                                    <div class="slds-truncate" title="Condition">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <ui:inputSelect class="slds-select" aura:id="rowcondition1" multiple="false" >
                                                    <aura:iteration items="{!v.row1condition}" var="row1condition_var"> 
                                                        <ui:inputSelectoption text="{!row1condition_var}"/>
                                                    </aura:iteration>
                                                </ui:inputSelect>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row" data-label="Filter Value(s)">
                                    <div class="slds-truncate" title="Filter Value(s)">
                                        <div class="slds-form-element">                                    
                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right" >
                                                <aura:if isTrue="{!v.type1 == 'number'}">
                                                    <ui:inputNumber aura:id="input1" class="slds-input" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type1 == 'search'}">
                                                    <div onclick="{!c.openOptionsPopup}" data-record="search1">
                                                        <a href="" class="noedit-onlysearch">
                                                            <lightning:icon iconName="utility:search" size="xx-small"></lightning:icon>
                                                        </a>
                                                    </div>
                                                    <ui:inputText aura:id="input1" class="slds-input avoid-click" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type1 == 'text'}">
                                                    <ui:inputText aura:id="input1" class="slds-input" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type1 == 'dropdown'}">
                                                    <ui:inputSelect class="slds-select" aura:id="input1" multiple="false" >
                                                        <aura:iteration items="{!v.type1Values}" var="type1Values_var"> 
                                                            <aura:if isTrue="{!type1Values_var == ''}">
                                                                <ui:inputSelectOption text="{!type1Values_var}" value="true" /> 
                                                                <aura:set attribute="else">
                                                                    <ui:inputSelectoption text="{!type1Values_var}"/>
                                                                </aura:set>
                                                            </aura:if>
                                                        </aura:iteration>
                                                    </ui:inputSelect>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type1 == 'date'}">
                                                    <ui:inputDate aura:id="input1" label="" value="" displayDatePicker="true" format="M/d/yyyy"/>
                                                </aura:if>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                
                                <td class="slds-cell-shrink" data-label="Sl.No">
                                    <div class="slds-truncate slds-p-left_small" title="Sl.No">2</div>
                                </td>
                                <th scope="row" data-label="Field">
                                    <div class="slds-truncate" title="Field">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <ui:inputSelect class="slds-select" aura:id="row2field" change="{!c.onFieldSelectChange}">
                                                    <aura:iteration items="{!v.client_Ref_dropDownVals}" var="client_Ref_DD_var"> 
                                                        <aura:if isTrue="{!client_Ref_DD_var == v.defSearchFieldScend}">
                                                            <ui:inputSelectOption text="{!client_Ref_DD_var}" value="true" /> 
                                                            <aura:set attribute="else">
                                                                <ui:inputSelectoption text="{!client_Ref_DD_var}" value=""/>
                                                            </aura:set>
                                                        </aura:if>
                                                    </aura:iteration>
                                                </ui:inputSelect>
												
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row" data-label="Condition">
                                    <div class="slds-truncate" title="Condition">
                                        <div class="slds-form-element">
                                            <div class="slds-form-element__control">
                                                <ui:inputSelect class="slds-select" aura:id="rowcondition2" multiple="false" >
                                                    <aura:iteration items="{!v.row2condition}" var="row2condition_var"> 
                                                        <ui:inputSelectoption text="{!row2condition_var}"/>
                                                    </aura:iteration>
                                                </ui:inputSelect>
                                            </div>
                                        </div>
                                    </div>
                                </th>
                                <th scope="row" data-label="Filter Value(s)">
                                    <div class="slds-truncate" title="Filter Value(s)">
                                        <div class="slds-form-element">                                    
                                            <div class="slds-form-element__control slds-input-has-icon slds-input-has-icon_right">
                                                <aura:if isTrue="{!v.type2 == 'number'}">
                                                    <ui:inputNumber aura:id="input2" class="slds-input" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type2 == 'search'}">
                                                    <div onclick="{!c.openOptionsPopup}" data-record="search2">
                                                        <a href="" class="noedit-onlysearch">
                                                            <lightning:icon iconName="utility:search" size="xx-small"></lightning:icon>
                                                        </a>
                                                    </div>
                                                    <ui:inputText aura:id="input2" class="slds-input avoid-click" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
                                                <aura:if isTrue="{!v.type2 == 'text'}">
                                                    <ui:inputText aura:id="input2" class="slds-input" value="" updateOn="keyup" keyup="{!c.enableGoBtn}" placeholder="Filter Values(s)" required="true"/>
                                                </aura:if>
