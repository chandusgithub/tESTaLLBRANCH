    <aura:attribute name="isSearchBtnClicked" type="boolean" default="false"/>
    
    <aura:attribute name="consultingFirmSearch" type="String[]"/>
    
    <aura:attribute name="testAttribute" type="String[]"/>
    
    <aura:attribute name="isSpinnertoLoad"  type="Boolean"  default="false"/>
    <aura:attribute name="isGoButtonClicked" type="Boolean" default="false"/>
    <aura:attribute name="isPrimaryExists" type="Boolean" default="false"/>
    <aura:attribute name="ErrorMessage" type="String"/>    
    <aura:attribute name="isAllContactList" type="boolean" default="false" />    
    <aura:attribute name="Child_Data" type="Object"/>    
    <aura:attribute name="isConsultantsListDisplayed" type="boolean" default="true" />
    <aura:attribute name="isConsultingFirmListDisplayed" type="boolean" default="false" />    
    <aura:attribute name="isNotSearched" type="boolean" default="true" />    
    <aura:attribute name="isSearchResultEmpty" type="boolean" default="false" />
    <aura:attribute name="isConsultantFirmsListEmpty" type="Boolean" default="true"/>
    <aura:attribute name="hideSelect" type="boolean" default="false" />
    <aura:attribute name="showSelect" type="boolean" default="false" />    
    <aura:handler name="pageChange1" event="c:PageChange" action="{!c.pageChange}"/>    
