                     <div class="slds-checkbox_add-button slds-text-align_center" data-aura-rendered-by="9:5864;a">
                         <input type="checkbox" aura:id="{!v.competitorsArray.Id}" data-category="{!v.competitorsArray.category}" data-record="Pharmacy" name="Pharmacy" id="{!v.competitorsArray.Id}" onclick="{!c.readCards}" checked="{!v.competitorsArray.isPharmacy}" disabled="{!v.competitorsArray.isPharmacyDisabled}" class="uiInput uiInputCheckbox uiInput--default uiInput--checkbox" data-aura-class="uiInput uiInputCheckbox uiInput--default uiInput--checkbox"></input>
