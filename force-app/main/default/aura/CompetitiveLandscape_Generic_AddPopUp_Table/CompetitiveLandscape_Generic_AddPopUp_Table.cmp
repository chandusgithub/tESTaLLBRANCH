                <div class="slds-checkbox_add-button slds-text-align_center" data-aura-rendered-by="9:5864;a">
                    <input type="checkbox" data-record="Medical" data-category="{!v.competitorsArray.category}" name="Medical" aura:id="{!v.competitorsArray.Id}" id="{!v.competitorsArray.Id}" checked="{!v.competitorsArray.isChecked}" onclick="{!c.readCards}" disabled="{!v.competitorsArray.disabled}" class="uiInput uiInputCheckbox uiInput--default uiInput--checkbox" data-aura-class="uiInput uiInputCheckbox uiInput--default uiInput--checkbox"></input> 
                    <label class="slds-checkbox_faux" for="add-checkbox-17" data-aura-rendered-by="18:5864;a">
                        <span class="slds-assistive-text" data-aura-rendered-by="19:5864;a">Select item 1</span>
                    </label>
