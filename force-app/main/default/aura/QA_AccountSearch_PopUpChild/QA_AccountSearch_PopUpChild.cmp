<aura:component access="global">
    
    <aura:attribute name="accountData" type="String"/>
    <aura:attribute name="IsAggregator" type="Boolean"/>
    
    <aura:registerEvent name="QA_AccountSearchPopUpEvent" type="c:QA_AccountSearch_PopUpEvent" />
    
    <tr class="slds-hint-parent rowData">
        <th scope="row" width="70">
            <div class="slds-truncate" title="Select">
                <a href="javascript:void()" onclick="{!c.selectRecord}">Select</a>
            </div>
        </th>
        <th scope="row" data-label="Last Name">
            <div class="slds-cell-wrap" title="Last Name">{!v.accountData.Name}</div>
        </th>
        <aura:if isTrue="{!v.IsAggregator == false}">
            <th scope="row" data-label="First Name">
                <div class="slds-cell-wrap" title="First Name">{!v.accountData.Category__c}</div>
            </th>
        </aura:if>
    </tr>
</aura:component>