    <aura:method name="makePrimaryUncheck" action="{!c.makePrimaryUncheck}" access="PUBLIC"></aura:method>
    <aura:attribute name="promptMessageText" type="String" />
    
    
    <div role="alertdialog" tabindex="-1" aura:id="confirmDeleteRecord" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse" onclick="{!c.confirmCancel}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">{!$Label.c.Confirmation}</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p class="slds-text-align--center">You are not allowed to edit this. Please Select through Date picker</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral" onclick="{!c.confirmDelete}">{!$Label.c.Delete}</button>
                <button class="slds-button slds-button--neutral" onclick="{!c.confirmCancel}">{!$Label.c.Cancel}</button>
            </div>
        </div>
    </div>
    
    
    <div role="alertdialog" tabindex="-1" aura:id="promptMessageForDateFields" aria-labelledby="prompt-heading-id"
         aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse"
                        onclick="{!c.confirmCancelForPrompt}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large" iconName="utility:close"
                                    size="x-small" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">Error</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p class="slds-text-align--center">{!v.promptMessageText}</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral"
                        onclick="{!c.confirmCancelForPrompt}">{!$Label.c.OK}</button>
            </div>
        </div>
    </div>
    <div class="slds-backdrop slds-backdrop--open slds-hide" aura:id="promptMessageForDateFields"></div>
    
    
    
    <div class="demo-only slds-hide" aura:id="role_Popup">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container" style="width: 40%;">
                <header class="slds-modal__header">
                    <button class="slds-button slds-button_icon slds-modal__close slds-button_icon-inverse close-btn" onclick="{!c.cancelRole}" title="Close">                        
                        <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    </button>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Role</h2>
                </header>
                <div class="slds-modal__content" id="modal-content-id-1">
                    <div  class = "slds-p-horizontal_medium slds-p-top_medium">
                        <div class=" slds-grid">
                            <div class="slds-col slds-size_5-of-7">
                                <ui:inputText class="slds-form-control" updateOn="keyup" aura:id="searchKeywordForUsers" keyup="{!c.enableGoBtn}" ></ui:inputText>
                            </div>
                            <div class="slds-col slds-size_1-of-7">
                                <div class="slds-p-left_x-small">
                                    <ui:button class="slds-button slds-button--brand" aura:id="goBtn" disabled="true" press="{!c.roleSearchField}">{!$Label.c.Go}</ui:button>
                                </div>
                            </div>
                            
                            <div class="slds-col slds-size_1-of-7"> 
                                
                                <ui:button class="slds-button slds-button--brand" aura:id="clrBtn" disabled="true" press="{!c.onClear}">{!$Label.c.Clear}</ui:button>
                                
                            </div>
                        </div>
                        <!--<lightning:input aura:id="userroleinput" name="searchText" onchange="{!c.roleSearchField}"  value="{!v.enteredSearchText}" type="search" class="leftspace"/>-->
                    </div>
                    <div class = "slds-p-around_medium"> 
                        <dl class="slds-list_horizontal slds-wrap slds-text-body_small">
                            <aura:if isTrue ="{!v.isRoleSearchEmpty == false}">
                                <aura:iteration items="{!v.roleSearchResult}" var="RolePicklistVar">
                                    <dt class="slds-item_label slds-text-color_weak slds-truncate slds-width_20"><a data-record="{!RolePicklistVar}" onclick="{!c.selectedRole}">Select</a></dt>
                                    <aura:if isTrue="{!RolePicklistVar == v.serviceAMTObj.Contact_Role__c}">
                                        <dd class="slds-item_detail slds-text-color_weak slds-truncate slds-width_80 selectedRole-Bgk autofocus">{!RolePicklistVar}</dd> 
                                        <aura:set attribute="else">
                                            <dd class="slds-item_detail slds-text-color_weak slds-truncate slds-width_80">{!RolePicklistVar}</dd> 
                                        </aura:set>
                                    </aura:if>                                                                                   
                                </aura:iteration>
                                <aura:set attribute="else"> 
                                    <div class="slds-align_absolute-center">
                                        <p>No Role found</p>
                                    </div>
                                </aura:set>
                            </aura:if>
                        </dl>
                    </div>
                </div>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </div>
    
    <aura:method name="removeProcessingIcon" action="{!c.removeProcessingOnCancel}" access="PUBLIC"></aura:method>
    
