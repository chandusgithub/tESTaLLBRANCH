                    <div class="slds-media__figure slds-grid slds-grid_vertical-align-center">
                        <button class="slds-button slds-text-body--regular action-bar-mobile" >
                            <lightning:icon class="icon-fallback-text sectionChevronIcon spin forceIcon" aura:id="utilityToggle" iconName="utility:chevronright" size="x-small" alternativeText="Product" />
                        </button>
                        <div aura:id="appletIcon">
                            <span class="slds-icon_container slds-icon-standard-contact" title="{!v.accountTypeCommonValues.AppletName}">
                                <lightning:icon class="slds-icon slds-icon--small" iconName="standard:contact" size="small" alternativeText="{!v.accountTypeCommonValues.AppletName}" />
                            </span>
                        </div>
                        <div aura:id="spinner" class="slds-hide dotsSpinnerContainer slds-is-relative">
                            <div class="dotsSpinnerContent slds-is-relative slds-p-around_medium">
                                <span class="slds-assistive-text">Loading</span>
                                <div role="status" class="slds-spinner slds-spinner_brand slds-spinner_x-small">                                
                                    <div class="slds-spinner__dot-a"></div>
                                    <div class="slds-spinner__dot-b"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="header-title">
                        <h2 class="slds-grid">
                            <!--<a href="javascript:void(0);" class="slds-card__header-link slds-truncate">-->
                            <span class="slds-text-heading--small">{!v.accountTypeCommonValues.AppletName}</span>
                            <aura:if isTrue="{!v.cdAccountType}">
                                <div class="slds-is-relative helptext">
                                    <c:tooltip2 helpLink="Help" helpText="{!$Label.c.Contacts_HelpText}"/>
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.cmAccountType}">
                                <div class="slds-is-relative helptext">
                                    <c:tooltip2 helpLink="Help" helpText="{!$Label.c.AccountContacts_HelpText}"/>
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.cfAccountType}">
                                <div class="slds-is-relative helptext">
                                    <c:tooltip2 helpLink="Help" helpText="{!$Label.c.CFConsultants_HelpText}"/>
                                </div>
                            </aura:if>
                            <!--</a>-->
                        </h2>
                    </div>
                    
                </header>
                <div class="appletAddBtn">
                    <aura:if isTrue="{!v.isDesktop}">
                        <aura:if isTrue="{!v.isLoggedInUserrole}">
                            <aura:if isTrue="{!v.showNewButton}">
                                <button class="slds-button slds-button--neutral" onclick="{!c.createRecord}">{!$Label.c.New}</button>
                            </aura:if>
                        </aura:if>
                        <aura:set attribute="else">
                            <div class="action-bar-mobile">
                                <aura:if isTrue="{!v.isLoggedInUserrole}">
                                    <aura:if isTrue="{!v.showNewButton}">
                                        <button class="slds-button slds-button--neutral" onclick="{!c.createRecord}">{!$Label.c.New}</button>
                                    </aura:if>
                                </aura:if>
                            </div>
                        </aura:set>
                    </aura:if>
                </div>
            </div>
            
            
            <div class="slds-section__content slds-is-relative">
