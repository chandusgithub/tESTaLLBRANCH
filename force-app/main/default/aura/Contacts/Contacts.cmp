<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" controller="CDAccounts">
    <aura:attribute name="cdContactList" type="Contact[]"/>
    <aura:attribute name="accountContacts" type="Contact[]"/>
    <aura:attribute name="ConsultantsList" type="Contact[]"/>
    <aura:handler name="init" action="{!c.doInit}" value="{!this}"/>
    <aura:attribute name='page' type='Integer' />
    <aura:attribute name='pages' type='Integer' />
    <aura:attribute name='total' type='Integer' />
    <aura:handler name="pageChange" event="c:PageChange" action="{!c.pageChange}"/>
    <aura:attribute name="accountTypeCommonValues" type="Object"/>
    <aura:attribute name="cdAccountType" type="Boolean" default="false"/>
    <aura:attribute name="cmAccountType" type="Boolean" default="false"/>
    <aura:attribute name="cfAccountType" type="Boolean" default="false"/>
    <aura:attribute name="contactRecordTypeId" type="Id"/>
    <aura:attribute name="sortConsultantTierAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortLastNameAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortFirstNameAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortJobTitleAsc" type="Boolean" default="true"/>
    <aura:attribute name="isAccountContactListEmpty" type="Boolean" default="false"/>
    <aura:attribute name="sortEmail" type="Boolean" default="true"/>
    <aura:attribute name="sortDecisionMakingRole" type="Boolean" default="true"/>
    <aura:attribute name="sortLastModifiedDate" type="Boolean" default="true"/> <!--SAMARTH-->
    <aura:attribute name="sortKeyContact" type="Boolean" default="true"/>
    <aura:attribute name="sortAMTLog" type="Boolean" default="true"/>
    <aura:attribute name="sortRoleAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortUhgRelationshipWithExecutiveAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortReportsToAsc" type="Boolean" default="true"/>
    <aura:attribute name="sortField" type="String" default="LastName"/>
    <aura:attribute name="ErrorMessage" type="String"/>
    <aura:attribute name="sortOrder" type="String" default="ASC"/>
    <aura:attribute name="isLoggedInUserrole" type="Boolean"/>
    <!--<aura:dependency resource="markup://force:recordSave" type="EVENT" />
    <aura:handler event="force:refreshView" action="{!c.getCDAccounts}" />-->
    <aura:handler event="c:GeneralInfoAccountApplicationEvent" action="{!c.setGeneralValues}"/>
    <aura:attribute name="isGenericEventTriggered" type="Boolean" default="false"/>
    <aura:attribute name="lastSortField" type="String" default=""/>
    <aura:attribute name="isScrollStop" type="Boolean" default="true"/>
    <aura:attribute name="isStop" type="Integer" default="1"/>
    <aura:attribute name="lastCount" type="Integer" default="1"/>
    <aura:attribute name="nextLastCount" type="Integer" default="1"/>
    <aura:attribute name="isExpand" type="Boolean" default="false"/>
    <aura:attribute name="isDesktop" type="Boolean" default="false"/>
    <aura:attribute name="scrollStyleForDevice" type="String" default="@media screen and (min-width: 320px) and (max-width: 1199px){.panel-content.scrollable{overflow-y: hidden !important;-webkit-overflow-scrolling: auto !important ;}}"/>
    <aura:attribute name="showNewButton" type="Boolean" default="false"/>    
    <aura:handler name="Sorting_Mobile_Event" event="c:Sorting_Mobile_Event" action="{!c.sortFieldsMobile}"/>
    
    <style>        
        {!v.scrollStyleForDevice}
        @media (min-width: 768px) and (max-width: 1199px) {
        .cuf-content {
        padding: 0 !important;
        }
        }
    </style>
    {!v.body}
    <div role="alertdialog" tabindex="-1" aura:id="ErrorMessage" aria-labelledby="prompt-heading-id" aria-describedby="prompt-message-wrapper" class="slds-modal slds-fade-in-open slds-modal--prompt slds-hide">
        <div class="slds-modal__container">
            <div class="slds-modal__header slds-theme--error slds-theme--alert-texture">
                <button class="slds-button slds-modal__close slds-button--icon-inverse" onclick="{!c.closeErrorModal}" title="Close">
                    <lightning:icon class="slds-button__icon slds-button__icon--large"  iconName="utility:close" size="x-small" alternativeText="Product" />
                    <span class="slds-assistive-text">{!$Label.c.Close}</span>
                </button>
                <h2 id="header43" class="slds-text-heading--medium">Error</h2>
            </div>
            <div class="slds-modal__content slds-p-around--medium">
                <p style="text-align:center;">{!v.ErrorMessage}</p>
            </div>
            <div class="slds-modal__footer slds-theme--default">
                <button class="slds-button slds-button--neutral buttonShadowBorder" onclick="{!c.closeErrorModal}">OK</button>
            </div>
        </div>
    </div>
    <div class="slds-backdrop slds-backdrop--open slds-hide" aura:id="ErrorMessage"></div>
    
    <div aura:id="acc_Contacts" class="slds-section">
        <article aura:id="articleClass" class="slds-card slds-card_boundary slds-is-relative" onscroll="{!c.scrollBottom}">
